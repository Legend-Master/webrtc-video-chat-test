{
    "files.associations": {
        ".parcelrc": "jsonc"
    },
    "json.schemas": [
        {
            "fileMatch": [
                ".parcelrc"
            ],
            "schema": {
                "allowTrailingCommas": true
            }
        }
    ],
    "css.lint.unknownAtRules": "ignore",
    "css.lint.validProperties": [
        "transition-behavior",
    ]
}